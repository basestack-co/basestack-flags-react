{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "paths": {
      "@basestack/flags-react": ["../../dist/index"],
      "@basestack/flags-react/client": ["../../dist/client"],
      "@basestack/flags-react/server": ["../../dist/server"]
    }
  },
  "include": ["routes", "app"],
  "exclude": ["node_modules"]
}
